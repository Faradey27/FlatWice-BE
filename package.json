{
  "name": "FlatWice-BE",
  "version": "0.0.1",
  "description": "FlatWice - app for rent",
  "scripts": {
    "dev": "npm run db && nodemon src/index.js --exec babel-node",
    "build": "babel -d ./dist ./src -s",
    "start": "npm run db && node dist/index.js",
    "lint": "eslint -c .eslintrc .",
    "test": "npm run db && jest --forceExit",
    "db": "node ./scripts/startDB.js"
  },
  "jest": {
    "testEnvironment": "node",
    "testURL": "http://0.0.0.0:8082",
    "setupFiles": [
      "./__test__/beforeTest.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "dependencies": {
    "axios": "0.16.2",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "1.18.2",
    "chalk": "2.1.0",
    "compression": "1.7.1",
    "connect-mongo": "2.0.0",
    "dotenv": "4.0.0",
    "errorhandler": "1.5.0",
    "express": "4.16.2",
    "express-flash": "0.0.2",
    "express-session": "1.15.6",
    "express-status-monitor": "1.0.1",
    "express-validator": "4.2.1",
    "mongoose": "4.12.2",
    "nodemailer": "4.2.0",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "supertest": "3.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-preset-env": "1.6.0",
    "eslint": "4.8.0",
    "eslint-config-hardcore-react": "0.3.0",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "jest": "21.2.1",
    "nodemon": "1.12.1",
    "sinon": "4.0.1",
    "sinon-mongoose": "2.0.2"
  },
  "main": "src/index.js",
  "keywords": [
    "react",
    "webpack2",
    "redux",
    "jest",
    "nextjs",
    "windows",
    "i18n"
  ],
  "author": {
    "name": "Andrii Tiertyshnyi",
    "email": "andrii.tiertyshnyi@gmail.com"
  },
  "contributors": [
    {
      "name": "Andrii Tiertyshnyi(Faradey27)",
      "url": "https://github.com/Faradey27"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Faradey27/FlatWice-BE"
  },
  "bugs": {
    "url": "https://github.com/Faradey27/FlatWice-BE/issues"
  },
  "engines": {
    "node": ">= 8.1.0"
  }
}
