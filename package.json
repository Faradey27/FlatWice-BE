{
  "name": "flatwice-be",
  "version": "0.0.1",
  "description": "FlatWice - app for rent",
  "scripts": {
    "dev": "npm run build && node ./node-runner/bin/watch",
    "start": "cross-env NODE_ENV=production npm run build && cross-env NODE_ENV=production node dist/index.js",
    "test": "jest --forceExit",
    "build": "node ./node-runner/bin/build",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "node ./node-runner/bin/debug",
    "feature:add": "node ./node-runner/bin/generateFeature"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/__test__/**/*.spec.ts",
      "__test__/**/*.spec.ts"
    ],
    "testEnvironment": "node",
    "testURL": "http://0.0.0.0:8082",
    "setupFiles": [
      "./__test__/beforeTest.ts"
    ],
    "mapCoverage": true,
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ],
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "dependencies": {
    "@types/sinon": "^2.3.7",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "1.18.2",
    "chalk": "^2.3.0",
    "compression": "1.7.1",
    "connect-mongo": "^2.0.0",
    "cross-env": "^5.1.1",
    "dotenv": "4.0.0",
    "errorhandler": "1.5.0",
    "express": "4.16.2",
    "express-flash": "0.0.2",
    "express-session": "1.15.6",
    "express-status-monitor": "^1.0.1",
    "express-validator": "4.3.0",
    "lusca": "^1.5.2",
    "mongoose": "4.13.0",
    "nodemailer": "4.3.1",
    "passport": "0.4.0",
    "passport-local": "1.0.0"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/body-parser": "^1.16.7",
    "@types/chalk": "^2.2.0",
    "@types/compression": "0.0.34",
    "@types/connect-mongo": "^0.0.34",
    "@types/dotenv": "^4.0.2",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.0.39",
    "@types/express-session": "1.15.5",
    "@types/jest": "^21.1.5",
    "@types/mongodb": "^2.2.15",
    "@types/mongoose": "^4.7.25",
    "@types/node": "^8.0.51",
    "@types/nodemailer": "^3.1.5",
    "@types/passport": "^0.3.5",
    "@types/request": "2.0.7",
    "@types/supertest": "^2.0.3",
    "concurrently": "^3.5.0",
    "jest": "^21.2.1",
    "nodemon": "^1.12.1",
    "request": "2.83.0",
    "shelljs": "^0.7.8",
    "sinon": "^4.1.2",
    "supertest": "^3.0.0",
    "ts-jest": "^21.2.1",
    "tslint": "^5.8.0",
    "tslint-config-airbnb": "^5.4.2",
    "typescript": "^2.6.2"
  },
  "keywords": [
    "react",
    "webpack2",
    "redux",
    "jest",
    "nextjs",
    "windows",
    "i18n"
  ],
  "author": {
    "name": "Andrii Tiertyshnyi",
    "email": "andrii.tiertyshnyi@gmail.com"
  },
  "contributors": [
    {
      "name": "Andrii Tiertyshnyi(Faradey27)",
      "url": "https://github.com/Faradey27"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Faradey27/FlatWice-BE"
  },
  "bugs": {
    "url": "https://github.com/Faradey27/FlatWice-BE/issues"
  },
  "engines": {
    "node": ">= 8.1.0"
  }
}
